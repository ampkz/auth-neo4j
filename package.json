{
	"name": "@ampkz/auth-neo4j",
	"version": "0.10.1",
	"description": "Simple Express middleware for session and user management using Neo4j.",
	"homepage": "https://github.com/ampkz/auth-neo4j#readme",
	"bugs": {
		"url": "https://github.com/ampkz/auth-neo4j/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ampkz/auth-neo4j.git"
	},
	"license": "GPL-3.0-or-later",
	"author": "Andrew M. Pankratz",
	"type": "commonjs",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./db": {
			"import": {
				"types": "./dist/api/utils/db.d.mts",
				"default": "./dist/api/utils/db.mjs"
			},
			"require": {
				"types": "./dist/api/utils/db.d.ts",
				"default": "./dist/api/utils/db.js"
			}
		},
		"./token": {
			"import": {
				"types": "./dist/api/utils/session.d.mts",
				"default": "./dist/api/utils/session.mjs"
			},
			"require": {
				"types": "./dist/api/utils/session.d.ts",
				"default": "./dist/api/utils/session.js"
			}
		},
		"./user": {
			"import": {
				"types": "./dist/users/user/user.d.mts",
				"default": "./dist/users/user.mjs"
			},
			"require": {
				"types": "./dist/users/user/user/d.ts",
				"default": "./dist/users/user.js"
			}
		},
		"./auth": {
			"import": {
				"types": "./dist/auth/auth.d.mts",
				"default": "./dist/auth/auth.mjs"
			},
			"require": {
				"types": "./dist/auth/auth.d.ts",
				"default": "./dist/auth/auth.js"
			}
		},
		"./session": {
			"import": {
				"types": "./dist/sessions/session.d.mts",
				"default": "./dist/sessions/session.mjs"
			},
			"require": {
				"types": "./dist/sessions/session.d.ts",
				"default": "./dist/sessions/session.js"
			}
		},
		"./errors": {
			"import": {
				"types": "./dist/errors/errors.d.mts",
				"default": "./dist/errors/errors.mjs"
			},
			"require": {
				"types": "./dist/errors/errors.d.ts",
				"default": "./dist/errors/errors.js"
			}
		},
		"./logger": {
			"import": {
				"types": "./dist/api/utils/logger.d.mts",
				"default": "./dist/api/utils/logger.mjs"
			},
			"require": {
				"types": "./dist/api/utils/logger.d.ts",
				"default": "./dist/api/utils/logger.js"
			}
		}
	},
	"scripts": {
		"prepare": "npm run build",
		"build": "tsup && fix-tsup-cjs",
		"test": "jest --coverage"
	},
	"devDependencies": {
		"@eslint/js": "^9.20.0",
		"@faker-js/faker": "^9.4.0",
		"@types/bcrypt": "^6.0.0",
		"@types/cookie-parser": "^1.4.8",
		"@types/express": "^5.0.3",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.13.1",
		"@types/supertest": "^6.0.2",
		"@types/winston": "^2.4.4",
		"eslint": "^9.20.1",
		"fix-tsup-cjs": "^1.2.0",
		"globals": "^15.15.0",
		"jest": "^29.7.0",
		"prettier": "^3.5.1",
		"supertest": "^7.0.0",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"tsup": "^8.4.0",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.24.1"
	},
	"peerDependencies": {
		"express": ">=5.1.0 <6.0.0",
		"neo4j-driver": ">=5.28.0 <6.0.0",
		"cookie-parser": ">=1.4.7 <2.0.0",
		"dotenv": ">=17.2.1 <18.0.0",
		"winston": ">=3.17.0 <4.0.0"
	},
	"dependencies": {
		"bcrypt": "^6.0.0"
	}
}
